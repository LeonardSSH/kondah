{
  "version": 2,
  "cli": { "defaultCollection": "@nrwl/node" },
  "projects": {
    "core": {
      "root": "libs/core",
      "sourceRoot": "libs/core/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["libs/core/**/*.ts"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/core"],
          "options": {
            "jestConfig": "libs/core/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/core",
            "tsConfig": "libs/core/tsconfig.lib.json",
            "packageJson": "libs/core/package.json",
            "main": "libs/core/src/index.ts",
            "assets": ["libs/core/*.md"]
          }
        }
      }
    },
    "examples-express-hbs": {
      "root": "apps/examples/express-hbs",
      "sourceRoot": "apps/examples/express-hbs/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/examples/express-hbs",
            "main": "apps/examples/express-hbs/src/main.ts",
            "tsConfig": "apps/examples/express-hbs/tsconfig.app.json",
            "assets": ["apps/examples/express-hbs/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/examples/express-hbs/src/environments/environment.ts",
                  "with": "apps/examples/express-hbs/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": { "buildTarget": "examples-express-hbs:build" }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/examples/express-hbs/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/examples/express-hbs"],
          "options": {
            "jestConfig": "apps/examples/express-hbs/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "plugins-http-controller": {
      "root": "libs/plugins/http-controller",
      "sourceRoot": "libs/plugins/http-controller/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/plugins/http-controller/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/plugins/http-controller"],
          "options": {
            "jestConfig": "libs/plugins/http-controller/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/plugins/http-controller",
            "tsConfig": "libs/plugins/http-controller/tsconfig.lib.json",
            "packageJson": "libs/plugins/http-controller/package.json",
            "main": "libs/plugins/http-controller/src/index.ts",
            "assets": ["libs/plugins/http-controller/*.md"]
          }
        }
      }
    },
    "adapters-express": {
      "root": "libs/adapters/express",
      "sourceRoot": "libs/adapters/express/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["libs/adapters/express/**/*.ts"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/adapters/express"],
          "options": {
            "jestConfig": "libs/adapters/express/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/adapters/express",
            "tsConfig": "libs/adapters/express/tsconfig.lib.json",
            "packageJson": "libs/adapters/express/package.json",
            "main": "libs/adapters/express/src/index.ts",
            "assets": ["libs/adapters/express/*.md"]
          }
        }
      }
    }
  }
}
