{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/node"
  },
  "projects": {
    "core": {
      "root": "libs/core",
      "sourceRoot": "libs/core/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/core/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/core"],
          "options": {
            "jestConfig": "libs/core/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/core",
            "tsConfig": "libs/core/tsconfig.lib.json",
            "packageJson": "libs/core/package.json",
            "main": "libs/core/src/index.ts",
            "assets": ["libs/core/*.md"]
          }
        }
      }
    },
    "express-adapter": {
      "root": "libs/express-adapter",
      "sourceRoot": "libs/express-adapter/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/express-adapter/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/express-adapter"],
          "options": {
            "jestConfig": "libs/express-adapter/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/express-adapter",
            "tsConfig": "libs/express-adapter/tsconfig.lib.json",
            "packageJson": "libs/express-adapter/package.json",
            "main": "libs/express-adapter/src/index.ts",
            "assets": ["libs/express-adapter/*.md"]
          }
        }
      }
    },
    "static-files-plugin": {
      "root": "libs/static-files-plugin",
      "sourceRoot": "libs/static-files-plugin/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/static-files-plugin/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/static-files-plugin"],
          "options": {
            "jestConfig": "libs/static-files-plugin/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/static-files-plugin",
            "tsConfig": "libs/static-files-plugin/tsconfig.lib.json",
            "packageJson": "libs/static-files-plugin/package.json",
            "main": "libs/static-files-plugin/src/index.ts",
            "assets": ["libs/static-files-plugin/*.md"]
          }
        }
      }
    },
    "http-controller": {
      "root": "libs/http-controller",
      "sourceRoot": "libs/http-controller/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/http-controller/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/http-controller"],
          "options": {
            "jestConfig": "libs/http-controller/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/http-controller",
            "tsConfig": "libs/http-controller/tsconfig.lib.json",
            "packageJson": "libs/http-controller/package.json",
            "main": "libs/http-controller/src/index.ts",
            "assets": ["libs/http-controller/*.md"]
          }
        }
      }
    },
    "examples-express-hbs": {
      "root": "apps/examples/express-hbs",
      "sourceRoot": "apps/examples/express-hbs/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/examples/express-hbs",
            "main": "apps/examples/express-hbs/src/main.ts",
            "tsConfig": "apps/examples/express-hbs/tsconfig.app.json",
            "assets": ["apps/examples/express-hbs/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/examples/express-hbs/src/environments/environment.ts",
                  "with": "apps/examples/express-hbs/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "examples-express-hbs:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/examples/express-hbs/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/examples/express-hbs"],
          "options": {
            "jestConfig": "apps/examples/express-hbs/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
